  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon --ignore './node_modules/' ./bin/www",
    "migrate": "node ./storage/migrate.js",
    "test": "ava --verbose"
  },
  "engines": {
    "node": ">=10.0.0 <13.0.0"
  },
  "dependencies": {
    "@phc/argon2": "^1.0.9",
    "better-sqlite3": "^5.4.2",
    "distributions": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "file-type": "^12.1.0",
    "morgan": "^1.9.1",
    "object-hash": "^1.3.1",
    "read-chunk": "^3.2.0",
    "upash": "^1.0.2"
  },
  "devDependencies": {
    "ava": "2.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^8.1.0",
    "nodemon": "^1.19.1"
  }
