  "private": true,
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ],
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "compileEnhancements": true
